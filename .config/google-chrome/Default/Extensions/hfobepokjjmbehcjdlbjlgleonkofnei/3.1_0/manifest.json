{
   "action": {
      "default_icon": "icons/icon48.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "description": "Changes the new tab to a page that lets user get instant dictionary definitions.",
   "externally_connectable": {
      "matches": [ "https://wordsfinder.xyz/*" ]
   },
   "icons": {
      "128": "icons/icon128.png",
      "48": "icons/button.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Words Finder",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.1"
}
